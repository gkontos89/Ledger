<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marshmallow.android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.AccountsHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.AccountSelectionActivity" />
        <activity android:name=".ui.CheckingAccountCreationActivity" />
        <activity android:name=".ui.CheckingAccountActivity" />
        <activity android:name=".ui.TransactionActivity" />
        <activity android:name=".ui.SummaryActivity"/>
        <activity android:name=".ui.BaseActivity" />
    </application>

</manifest>